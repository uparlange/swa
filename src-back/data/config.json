{
    "expressPort": "8080",
    "passportSecretOrKey": "mySuperSecretKey",
    "expressStaticsViewsConf": {
        "folder": "src-front/views",
        "engine": "pug",
        "files": [
            {
                "path": "/",
                "value": "index.pug"
            },
            {
                "path": "/load-offline-cache",
                "value": "load-offline-cache.pug"
            }
        ]
    },
    "expressStaticsLocalesConf": {
        "folder": "src-back/data/i18n",
        "path": "/data/i18n",
        "files": [
            "en.json",
            "fr.json"
        ]
    },
    "expressStaticsVendorsConf": {
        "path": "/vendors",
        "vendors": [
            {
                "folder": "node_modules/vue/dist",
                "files": [
                    "vue.min.js"
                ]
            },
            {
                "folder": "node_modules/vue-router/dist",
                "files": [
                    "vue-router.min.js"
                ]
            },
            {
                "folder": "node_modules/vue-resource/dist",
                "files": [
                    "vue-resource.min.js"
                ]
            },
            {
                "folder": "node_modules/vue-i18n/dist",
                "files": [
                    "vue-i18n.min.js"
                ]
            },
            {
                "folder": "node_modules/vuetify/dist",
                "files": [
                    "vuetify.min.css",
                    "vuetify.min.js"
                ]
            },
            {
                "folder": "node_modules/socket.io-client/dist",
                "files": [
                    "socket.io.js"
                ]
            },
            {
                "folder": "node_modules/material-design-icons/iconfont",
                "files": [
                    "MaterialIcons-Regular.eot",
                    "MaterialIcons-Regular.woff2",
                    "MaterialIcons-Regular.woff",
                    "MaterialIcons-Regular.ttf"
                ]
            }
        ]
    }
}