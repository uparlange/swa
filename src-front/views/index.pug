html

head

    title!= title

    meta(charset="utf-8")

    meta(name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no,minimal-ui")
    meta(name="mobile-web-app-capable" content="yes")
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="msapplication-config" content="/images/favicon/browserconfig.xml")
    meta(name="theme-color" content="#ffffff")

    link(rel="apple-touch-icon" sizes="180x180" href="/images/favicon/apple-touch-icon.png")
    link(rel="icon" type="image/png" sizes="32x32" href="/images/favicon/favicon-32x32.png")
    link(rel="icon" type="image/png" sizes="16x16" href="/images/favicon/favicon-16x16.png")
    link(rel="manifest" href="/images/favicon/manifest.json")
    link(rel="mask-icon" href="/images/favicon/safari-pinned-tab.svg" color="#5bbad5")
    link(rel="shortcut icon" href="/images/favicon/favicon.ico")

    each vendor in vendorsConf.vendors
        each file in vendor.files
            if file.indexOf(".css") !== -1
                link(rel="stylesheet" href=vendorsConf.path+"/"+file)
            else if file.indexOf(".js") !== -1
                script(type="text/javascript" src=vendorsConf.path+"/"+file)

    link(rel="stylesheet" href="/css/application.css")
    
    script(type="text/javascript" src="/js/fwk.js")
    script(type="text/javascript" src="/js/application.js")
    script(type="text/javascript" src="/js/bootstrap.js")

body

    div(id="MainView")